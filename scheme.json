{
  "name": "justFM",

  "buildDir": "bin",

  "compilerOptions": {
    "compilationLevel": "SIMPLE_OPTIMIZATIONS",
    "warningLevel": "VERBOSE",
    "language": "ECMASCRIPT5",
    "formatting": ""
  },

  "srcDir": "lib",
  "rootNamespace": "fm",

  "modules": {
    "fm": {
      "src": [
        "fm.js",
        "i-accumulator.js",
        "i-iterator.js",
        "atom-accumulator.js",
        "list-accumulator.js",
        "array-iterator.js",
        "list/list.js",
        "acts/acts.js",
        "log/log.js"
      ]
    }
  },

  "externsDir": "externs",

  "deps": {
    "justAM": "https://github.com:kalachevmax/justAM"
  }
}
